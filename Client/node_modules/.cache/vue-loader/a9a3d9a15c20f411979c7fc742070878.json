navbar-nav class=\"ml-auto\">\r\n              <router-link class=\"nav-link\" to=\"/body-map\" >Body Map</router-link>\r\n                <router-link class=\"nav-link\" to=\"/exercises\" >Exercises</router-link>\r\n                <router-link class=\"nav-link\" to=\"/manage-exercises\" >Manage Exercises</router-link>\r\n              <b-nav-item-dropdown right>\r\n                <!-- Using 'button-content' slot -->\r\n                <template #button-content>\r\n                  <b-icon icon=\"person-circle\" aria-hidden=\"true\"></b-icon>\r\n                </template>\r\n                <b-dropdown-item router-link class=\"nav-link\" to=\"/user\" v-bind:userId=\"this.user\">Profile</b-dropdown-item>\r\n                <b-dropdown-item router-link class=\"nav-link\" to=\"/login\" v-on:click=\"logOut()\">Sign Out</b-dropdown-item>\r\n              </b-nav-item-dropdown>\r\n            </b-navbar-nav>\r\n          </b-collapse>\r\n        </b-navbar>\r\n      </div>\r\n          <!-- Render the content of the current page view -->\r\n          <router-view v-bind:user=\"user\" />\r\n        </div>\r\n\r\n    <div v-else>\r\n      <div v-if=\"signIn\">\r\n        <sign-in @login=\"login()\" />\r\n        <button @click=\"switchToSignUp()\">Sign up instead</button>\r\n      </div>\r\n      <div v-else>\r\n        <sign-up @login=\"login()\" />\r\n        <button @click=\"switchToSignIn()\">Sign in instead</button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { Api } from '@/Api'\r\nimport signIn from './views/SignIn.vue'\r\nimport signUp from './views/SignUp.vue'\r\nimport Header from '@/components/Header.vue'\r\n\r\nexport default {\r\n  components: {\r\n    // Define the name of the component here\r\n    'menu-header': Header,\r\n    'sign-in': signIn,\r\n    'sign-up': signUp\r\n  },\r\n  data() {\r\n    return {\r\n      isLoggedIn: false,\r\n      user: {},\r\n      signIn: true\r\n    }\r\n  },\r\n  mounted() {\r\n    if (localStorage.getItem('token') === null) {\r\n      this.isLoggedIn = false\r\n      this.$router.push('/login')\r\n    } else {\r\n      this.isLoggedIn = true\r\n      this.getUser()\r\n    }\r\n  },\r\n  methods: {\r\n    login() {\r\n      this.isLoggedIn = true\r\n      this.getUser()\r\n      console.log(this.user)\r\n      this.$router.push('/')\r\n    },\r\n    getUser() {\r\n      Api.get('/user', {\r\n        headers: { token: localStorage.getItem('token') }\r\n      }).then((res) => {\r\n        console.log(res.data.user._id)\r\n        this.user = res.data.user._id\r\n      })\r\n    },\r\n    switchToSignUp() {\r\n      this.signIn = false\r\n    },\r\n    switchToSignIn() {\r\n      this.signIn = true\r\n    },\r\n    logOut() {\r\n      localStorage.setItem('token', null)\r\n      this.isLoggedIn = false\r\n      this.user = {}\r\n      localStorage.clear()\r\n      this.$router.push('/login')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n#app {\r\n  font-family: 'Avenir', Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  text-align: center;\r\n  color: #2c3e50;\r\n}\r\n#nav {\r\n  padding: 1rem 1.5rem;\r\n  background-color: #F5793B;\r\n}\r\n.nav-link{\r\n    font-size: 1rem;\r\n    font-weight: 500;\r\n    color: #475569;\r\n}\r\n.navbar {\r\n  font-style: bold;\r\n  background-color: #F5793B;\r\n  font-family: Arial, sans-serif;\r\n  font-weight: 400;\r\n}\r\n.navbar-nav {\r\n  margin-left: 30px;\r\n  margin-right: 30px;\r\n}\r\n\r\n</style>\r\n"]}]}